{
  "name": "Arizen",
  "version": "1.1.4",
  "description": "Arizen is light API wallet for ZenCash with localy stored and encrypted files.",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node .electron-vue/dev-runner.js",
    "dist": "npm run pack:main && npm run pack:renderer && electron-builder",
    "pack": "npm run pack:main && npm run pack:renderer && electron-builder --dir",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": ""
  },
  "homepage": "https://github.com/ZencashOfficial/arizen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zencashofficial/arizen.git"
  },
  "bugs": {
    "url": "https://github.com/zencashofficial/arizen/issues"
  },
  "updater": {
    "url": "https://raw.githubusercontent.com/ZencashOfficial/arizen/master/updates.json"
  },
  "keywords": [
    "zen",
    "zencash",
    "equihash",
    "wallet",
    "windows",
    "mac",
    "osx",
    "linux",
    "desktop",
    "electron"
  ],
  "author": "ZEN team <lukas@zensystem.io>",
  "contributors": [
    {
      "name": "@lukas",
      "email": "lukas@zensystem.io"
    },
    {
      "name": "@nonghost",
      "email": "neduchal@gmail.com"
    },
    {
      "name": "@k4chn1k",
      "email": "k4chn1k@gmail.com"
    },
    {
      "name": "@woky",
      "email": "nechtom@gmail.com"
    },
    {
      "name": "@gsfakianakis",
      "email": "g.e.sfakianakis@gmail.com"
    },
    {
      "name": "@Tigger",
      "email": "-"
    },
    {
      "name": "@magg0t",
      "email": "magg0t@protonmail.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=6.0"
  },
  "build": {
    "productName": "Arizen",
    "appId": "io.zensystem.arizen",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "mac": {
      "category": "public.app-category.finance",
      "type": "distribution",
      "icon": "build/icons/icon.icns"
    },
    "dmg": {
      "title": "${productName} ${version}",
      "icon": "build/icons/icon.icns",
      "backgroundColor": "#888888",
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/icons",
      "category": "Office"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    }
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bip32-utils": "^0.11.0",
    "bitcoinjs-lib": "^3.3.1",
    "copy-to-clipboard": "^3.0.8",
    "electron-simple-updater": "^1.2.3",
    "element-ui": "^2.2.0",
    "fs-extra": "^5.0.0",
    "luxon": "^0.4.0",
    "password-hash": "^1.2.2",
    "qrcode": "^1.2.0",
    "repeat": "^0.0.6",
    "request": "^2.83.0",
    "store": "^2.0.12",
    "sql.js": "^0.5.0",
    "node-fetch": "^2.0.0",
    "jspdf": "^1.3.5",
    "vue": "^2.5.13",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vue-i18n": "^7.4.2",
    "zencashjs": "^1.1.9-a",
    "js-vue-logger": "^1.4.3",
    "encryption": "file:lib/encryption",
    "api": "file:lib/api",
    "async-request": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "less-loader": "^4.0.5",
    "less": "^3.0.1",
    "del": "^3.0.0",
    "ajv": "^5.5.1",
    "coveralls": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.8.2-beta.5",
    "electron-builder": "^19.19.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
